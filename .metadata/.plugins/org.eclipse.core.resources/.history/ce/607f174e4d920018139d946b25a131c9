package GUI;

import java.util.ArrayList;

import javax.swing.SwingWorker;

import Logic.DijkstraAlgorithm;

public class AlgorithmWorker extends SwingWorker<Integer, Integer> {

	private Modell modell;
	private String algorithm;
	
	public AlgorithmWorker(Modell modell, String Algorithm) {
		this.modell = modell;
		this.algorithm = Algorithm;
	}
	
	@Override
	protected Integer doInBackground() throws Exception {

		

		
		//test:
		DijkstraAlgorithm testAlgo = new DijkstraAlgorithm(1,8, modell.getHighWayGraph());
		testAlgo.startAlgorithm();

		ArrayList<Integer > resultTest = testAlgo.getResultList();
		System.out.println("Min Distance is: " + testAlgo.getMinDistance());
		System.out.print("Reihenfolge der Knoten ist: ");
		for (int t : resultTest) {
			System.out.print(t + "; ");
		}
		//test end
		
		return null;
	}

}

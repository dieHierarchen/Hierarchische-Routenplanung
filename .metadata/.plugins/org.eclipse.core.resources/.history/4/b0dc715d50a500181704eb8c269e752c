package Graph;

public class Edge {

	private Node start;
	private Node aim;
	private double weight;

	public Edge(Node start, Node aim) {

		this.start = start;
		this.aim = aim;

	}

	public double getWeight() {
		double weight = this.weight;
		return weight;
	}

	public Node getAim() {
		Node aim = this.aim;
		return aim;
	}

	public Node getStart() {
		Node start = this.start;
		return start;
	}

}

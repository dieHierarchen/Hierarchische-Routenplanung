package UnitTests;

import org.junit.*;

public class UnitTestDijkstra {
	
	private int[][] testGraph;
	
	@Before
	public void setpUp() {
		//setting up a testing matrix (incidence list of a graph)
		testGraph = new int[10][10];
		for (int i = 0; i < testGraph.length; i++) {
			for (int k = 0; k < testGraph.length; k++) {			//initial =0
				testGraph[i][k] = 0;
			}
		}
		
		//adding edges/values:
		testGraph[1][3] = 5;
		testGraph[1][2] = 6;
		
		testGraph[2][1] = 6;
		testGraph[2][4] = 3;
		testGraph[2][8] = 25;
		testGraph[2][5] = 10;
		
		testGraph[3][1] = 5;
		testGraph[3][4] = 7;
		
		testGraph[a][b] = c;
	}
	
	@Test
	public void run() {
		
	}
}

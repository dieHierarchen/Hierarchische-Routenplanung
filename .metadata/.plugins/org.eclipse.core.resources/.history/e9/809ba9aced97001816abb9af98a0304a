package GUI;

import javax.swing.SwingWorker;

public class CommercialWoker extends SwingWorker<Integer, Integer>{

	private volatile boolean addBlock;
	
	public CommercialWoker() {
		addBlock = false;
	}

	
	@Override
	protected Integer doInBackground() throws Exception {
		
		while (!addBlock) {
			
		}
		
		return null;
	}

}

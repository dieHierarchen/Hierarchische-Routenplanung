package Graph;

public class Edge {

	private Node start;
	private Node aim;
	private int weight;

	public Edge(Node s, Node a, int w) {

		start = s;
		aim = a;
		weight = w;

	}

	public int getWeight() {
		int x = this.weight;
		return x;
	}

	public Node getAim() {
		Node n = this.aim;
		return n;
	}

	public Node getStart() {
		Node n = this.start;
		return n;
	}

}
